    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug WebCrawler",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/WebCrawler/cmd",
            "args": [
                "-domain=https://a5e.tools",
                "-startURL=https://a5e.tools/spells"
            ]
        },
        {
            "name": "Debug FileCrawler",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/FileCrawler/cmd",
            "args": [
                "-folder=../resources/html"
            ]
        }
    ]
}